{
  "compilerOptions": {
    "baseUrl": "src",
    "outDir": "./build/",
    "paths": {
      "Components": ["components"],
      "Assets": ["assets"],
      "Helpers": ["helpers"],
      "Pages": ["pages"],
      "Types": ["types"],
    },
    "noImplicitAny": true,
    "sourceMap": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true
  }
}